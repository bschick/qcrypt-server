    "@simplewebauthn/types": "^9.0.1",
    "@types/node": "^20.11.16",
    "esbuild": "^0.20.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.511.0",
    "@simplewebauthn/server": "^9.0.1",
    "electrodb": "^2.13.1"
  }
}
